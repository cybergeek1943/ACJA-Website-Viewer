; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Asphaleia Browser"
run/main_scene="res://scenes/base.tscn"
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=500
boot_splash/image="res://ui/graphics/boot.png"
boot_splash/fullsize=false
config/icon="res://ui/icons/application_icon.png"
config/windows_native_icon="res://ui/icons/application_icon.ico"

[autoload]

Global="*res://scenes/global/global.gd"
Website="*res://scenes/global/website.gd"
Email="*res://scenes/global/email.gd"
InternetConnection="*res://scenes/internet_connection_checker.gd"
Description="*res://scenes/global/descriptions.gd"

[display]

window/size/resizable=false
window/size/borderless=true
mouse_cursor/custom_image="res://ui/cursors/1.png"

[importer_defaults]

image={
}

